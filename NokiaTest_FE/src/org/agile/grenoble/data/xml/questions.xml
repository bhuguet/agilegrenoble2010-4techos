	<org.agile.grenoble:Question>
		<org.agile.grenoble:label>Q1 : How long are your Iterations</org.agile.grenoble:label>
		<org.agile.grenoble:answers>
			<org.agile.grenoble:configuration>
				<org.agile.grenoble:type>single</org.agile.grenoble:type>
            </org.agile.grenoble:configuration>
			<org.agile.grenoble:answer>
				<org.agile.grenoble:label>No Iteration</org.agile.grenoble:label>
				<org.agile.grenoble:point>0</org.agile.grenoble:point>
            </org.agile.grenoble:answer>
			<org.agile.grenoble:answer>
				<org.agile.grenoble:label>Iteration &gt; 6 weeks </org.agile.grenoble:label>
				<org.agile.grenoble:point>1</org.agile.grenoble:point>
            </org.agile.grenoble:answer>
			<org.agile.grenoble:answer>
				<org.agile.grenoble:label>Iteration duration is variable and lt; 6 weeks</org.agile.grenoble:label>
				<org.agile.grenoble:point>2</org.agile.grenoble:point>
            </org.agile.grenoble:answer>
			<org.agile.grenoble:answer>
				<org.agile.grenoble:label>Fixed Iteration of 6 weeks </org.agile.grenoble:label>
				<org.agile.grenoble:point>3</org.agile.grenoble:point>
            </org.agile.grenoble:answer>
			<org.agile.grenoble:answer>
				<org.agile.grenoble:label>Fixed Iteration length of 5 weeks</org.agile.grenoble:label>
				<org.agile.grenoble:point>4</org.agile.grenoble:point>
            </org.agile.grenoble:answer>
			<org.agile.grenoble:answer>
				<org.agile.grenoble:label>Fixed iteration length of 4 weeks or less</org.agile.grenoble:label>
				<org.agile.grenoble:point>10</org.agile.grenoble:point>
            </org.agile.grenoble:answer>
        </org.agile.grenoble:answers>
    </org.agile.grenoble:Question>

	
